    "debug": "cross-env-shell NODE_ENV=debug \"npm run typecheck && node scripts/build.mjs && vite ./src/renderer\"",
    "eslint": "eslint --ext .ts,.js ./",
    "prettier": "prettier --write './**/*.{ts,js,tsx,jsx}'",
